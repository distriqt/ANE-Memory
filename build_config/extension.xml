<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.Memory</id>
	<versionNumber>@VERSION@</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.memory.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.memory.MemoryExtension</initializer>
				<finalizer>com.distriqt.extension.memory.MemoryExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM64">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.memory.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.memory.MemoryExtension</initializer>
				<finalizer>com.distriqt.extension.memory.MemoryExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.memory.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.memory.MemoryExtension</initializer>
				<finalizer>com.distriqt.extension.memory.MemoryExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libMemory.a</nativeLibrary>
				<initializer>MemoryExtInitializer</initializer>
				<finalizer>MemoryExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libMemory.a</nativeLibrary>
				<initializer>MemoryExtInitializer</initializer>
				<finalizer>MemoryExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
